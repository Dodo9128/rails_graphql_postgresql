<h1>Users</h1>

<table>
  <thead>
    <tr>
      <th>First Name</th>
      <th>Last Name</th>
    </tr>
  </thead>

  <tbody>
    <% @users.each do |users| %>
      <tr>
        <td><%= users.first_name %></td>
        <td><%= users.last_name %></td>
      <td><%= link_to "#{users.first_name} #{users.last_name}'s Books", "/author/#{users.id}" %>
      </tr>
    <% end %>
  </tbody>
  <td><%=link_to "Add New User", "/signin/new" %></td>
</table>
  <td><%=link_to "Go To Main", "/" %></td>

<br>

